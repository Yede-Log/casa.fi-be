{
  "name": "otaas-ts-api-template-repo",
  "version": "1.0.0",
  "description": "Typescript based API template",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --pretty --transpile-only src/index.ts | bunyan",
    "build": "tsc -p .",
    "ts.check": "tsc --project tsconfig.json",
    "add-build": "git add dist",
    "start": "mkdir -p ./logs/ && node ./dist/index.js | bunyan"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OTaaS/otaas-ts-api-template-repo.git"
  },
  "author": "Sangat Das",
  "license": "SEE LICENSE IN LICENSE",
  "bugs": {
    "url": "https://github.com/OTaaS/otaas-ts-api-template-repo/issues"
  },
  "homepage": "https://github.com/OTaaS/otaas-ts-api-template-repo#readme",
  "dependencies": {
    "@pushprotocol/restapi": "^1.4.44",
    "body-parser": "^1.19.0",
    "bunyan": "^1.8.15",
    "cors": "^2.8.5",
    "ethers": "^5.7.0",
    "express": "^4.17.1",
    "helmet": "^4.3.1",
    "jsonwebtoken": "^9.0.2",
    "jwt-simple": "^0.5.6",
    "mongoose": "^5.13.21",
    "moralis": "^2.23.2"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/bunyan": "^1.8.6",
    "@types/cookie-parser": "^1.4.6",
    "@types/cors": "^2.8.9",
    "@types/express": "^4.17.21",
    "@types/jwt-simple": "^0.5.33",
    "@types/mongodb": "^4.0.7",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^14.14.21",
    "@types/passport": "^1.0.5",
    "@types/passport-jwt": "^3.0.3",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.3.1",
    "pre-commit": "^1.2.2",
    "rimraf": "^5.0.5",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.1.3"
  }
}
